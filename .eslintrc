{
    "env": {
      "node": true,
      "es6": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:jest/recommended"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 8,
      "sourceType": "module",
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true
      }
    },
    "rules": {
      "semi": "error",
      "indent": [
        "error",
        4,
        {
          "SwitchCase": 1
        }
      ],
      "no-useless-escape": 0,
      "no-unused-vars": 0,
      "no-redeclare": 0,
      "no-inner-declarations": 0,
      "no-console": 0,
      "no-debugger": 0
    },
    "plugins": [
        "jest"
    ]
  }
  